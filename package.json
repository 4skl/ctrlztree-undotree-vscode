{
  "name": "ctrlztree",
  "displayName": "CtrlZTree",
  "description": "History of modifications in the form of a tree instead of a stack",
  "version": "0.1.0",
  "publisher": "4skl",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/4skl/CtrlZTree.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onStartupFinished"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ctrlztree.undo",
        "title": "CtrlZTree: Undo"
      },
      {
        "command": "ctrlztree.redo",
        "title": "CtrlZTree: Redo"
      },
      {
        "command": "ctrlztree.visualize",
        "title": "CtrlZTree: Visualize History Tree"
      }
    ],
    "keybindings": [
      {
        "command": "ctrlztree.undo",
        "key": "ctrl+z",
        "mac": "cmd+z",
        "when": "editorTextFocus"
      },
      {
        "command": "ctrlztree.redo",
        "key": "ctrl+y",
        "mac": "cmd+y",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
