{
  "name": "ctrlztree",
  "displayName": "CtrlZTree",
  "description": "History of modifications in the form of a tree instead of a stack, like in the Undotree plugin.",
  "keywords": [
    "vscode",
    "extension",
    "undotree",
    "history",
    "tree",
    "ctrlz"
  ],
  "version": "0.5.2",
  "publisher": "4skl",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/4skl/ctrlztree-undotree-vscode.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ctrlztree.undo",
        "title": "CtrlZTree: Undo"
      },
      {
        "command": "ctrlztree.redo",
        "title": "CtrlZTree: Redo"
      },
      {
        "command": "ctrlztree.visualize",
        "title": "CtrlZTree: Visualize History Tree"
      }
    ],
    "keybindings": [
      {
        "command": "ctrlztree.undo",
        "key": "ctrl+z",
        "mac": "cmd+z",
        "when": "editorTextFocus"
      },
      {
        "command": "ctrlztree.redo",
        "key": "ctrl+y",
        "mac": "cmd+y",
        "when": "editorTextFocus"
      },
      {
        "command": "ctrlztree.redo",
        "key": "ctrl+shift+z",
        "mac": "cmd+shift+z",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.18.63",
    "@types/vscode": "^1.60.1",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^11.5.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "files": [
    "out/**",
    "resources/**",
    "src/webview/**",
    "package.json",
    "README.md"
  ]
}
